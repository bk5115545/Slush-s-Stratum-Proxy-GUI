package crypto.currency.workers;

public class Worker {
	
	String name = "default";
	int downloaded = 0;
	int submitted = 0;
	int accepted = 0;
	int rejected = 0;
	
	int currentDiff = 0;
	
	long startTime = 0;
	protected Worker(String name) {
		this.name = name;
		startTime = System.currentTimeMillis();
	}
	
	

}
